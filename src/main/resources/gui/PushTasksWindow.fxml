<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <fx:define>
        <Double fx:id="BORDER_SPACING" fx:value="5.0"/>
    </fx:define>
    <ScrollPane fx:id="scrollPane" fitToWidth="true"
                AnchorPane.topAnchor="$BORDER_SPACING" AnchorPane.bottomAnchor="$BORDER_SPACING"
                AnchorPane.leftAnchor="$BORDER_SPACING" AnchorPane.rightAnchor="$BORDER_SPACING">
        <VBox fx:id="prevDayTasksHolder" spacing="-2.0" />
    </ScrollPane>

</AnchorPane>
