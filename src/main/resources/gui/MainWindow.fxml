<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.ScrollPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="mainPane"
            stylesheets="@MainWindow.css">
    <fx:define>
        <Double fx:id="BORDER_SPACING" fx:value="5.0"/>
    </fx:define>
    <ScrollPane fx:id="scrollPane" fitToWidth="true"
            AnchorPane.topAnchor="$BORDER_SPACING" AnchorPane.bottomAnchor="$BORDER_SPACING"
            AnchorPane.leftAnchor="$BORDER_SPACING" AnchorPane.rightAnchor="$BORDER_SPACING">
        <VBox fx:id="wrapperBox" spacing="-2.0">
                <VBox fx:id="taskListGui" spacing="-2.0" />
        </VBox>
    </ScrollPane>

</AnchorPane>
